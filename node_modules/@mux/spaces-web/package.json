{
  "name": "@mux/spaces-web",
  "version": "0.2.2",
  "scripts": {
    "prebuild": "genversion --es6 --semi src/version.ts",
    "build": "rm -rf dist && yarn build:esm && yarn build:cjs && yarn build:iife && yarn build:types",
    "build:esm": "esbuild src/index.ts --target=es2019 --bundle --minify --keep-names --format=esm --outdir=dist --out-extension:.js=.mjs --define:process.env.DEV=false --define:process.env.BACKEND_URL=\\\"\\\"",
    "build:cjs": "esbuild src/index.ts --target=es2019 --bundle --minify --keep-names --format=cjs --outdir=dist --out-extension:.js=.cjs --define:process.env.DEV=false --define:process.env.BACKEND_URL=\\\"\\\"",
    "build:iife": "esbuild src/index.ts --target=es2019 --bundle --minify --keep-names --format=iife --outfile=dist/mux-spaces.js --global-name=MuxSpaces --define:process.env.DEV=false --define:process.env.BACKEND_URL=\\\"\\\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "example": "yarn prebuild && DEV=true NODE_ENV=staging parcel ./example/index.html --port 3001",
    "example-local": "yarn prebuild && DEV=true parcel ./example/index.html --port 3001",
    "example-prod": "yarn prebuild && DEV=true NODE_ENV=production parcel ./example/index.html --port 3001",
    "lint": "eslint src/* && prettier --check src/*",
    "lint-fix": "prettier --write src/* example/*",
    "docs": "typedoc",
    "jest": "jest test/suites/integration test/suites/unit",
    "jest-test-coverage": "jest test/suites/integration test/suites/unit --coverage --collectCoverageFrom=\"src/**/*\"",
    "playwright": "playwright install && playwright test --project=chromium --project=firefox",
    "playwright-staging": "playwright install && NODE_ENV=staging playwright test --project=chromium --project=firefox",
    "playwright-local": "playwright install && NODE_ENV=local playwright test --project=chromium --project=firefox",
    "playwright-prod": "playwright install && NODE_ENV=prod playwright test --project=chromium --project=firefox",
    "dev": "./dev.sh",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@parcel/packager-ts": "^2.4.0",
    "@parcel/transformer-typescript-types": "^2.4.0",
    "@playwright/test": "^1.22.2",
    "@types/events": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.22",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "axios": "^0.27.2",
    "babel-jest": "^27.5.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.38",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "genversion": "^3.1.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jest-websocket-mock": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "mock-socket": "^9.1.2",
    "parcel": "^2.4.0",
    "prettier": "^2.6.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.13",
    "typedoc-plugin-no-inherit": "^1.3.1",
    "typescript": "^4.6.2",
    "util": "^0.12.4"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/mux-spaces.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "files": [
    "/dist"
  ],
  "targets": {
    "module": {
      "includeNodeModules": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "dependencies": {
    "@sentry/browser": "^7.1.1",
    "@sentry/tracing": "^7.1.1",
    "@testrtc/watchrtc-sdk": "^1.35.1",
    "events": "^3.3.0",
    "uuid": "^8.3.2",
    "webrtc-adapter": "^8.1.1"
  }
}
